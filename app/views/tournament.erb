        <div class="outer team">
          <p class="w2"></p>
        </div>
      </div>
    </div>
    <div class="round winner">
      <div class="outer team">
          <p class="w3"></p>
        </div>
    </div>
    <div class="round consolation">
      <div class="matchup m4">
