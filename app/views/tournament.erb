<div id="tournament">
  <!-- <button id="seed">Enter Ideas</button> -->
  <div id="tournament-controls">
    <div>
      <input id="company-name" value="Company Name" maxlength="35">
      <button id="company-button">Enter</button>
    </div>
    <div>
      <input id="goal-field" value="Goal" maxlength="70">
      <button id="goal-button">Enter</button>
    </div>
    <div>
      <input id="seed" value="Enter Idea" maxlength="60">
      <button id="add">Add Idea</button>
      <button id="begin">Begin Tournament</button>
    </div>
  </div>
  <div id="tournament-details">
    <h1 id="company-header"></h1>
    <h2 id="goal-header"></h2>
  </div>
  <ul id="list">
    <h1>Ideas</h1>
  </ul>

  <ul id="winners">
    <li id="winners-first">First Place:</li>
    <li id="winners-second">Second Place:</li>
    <li id="winners-third">Third Place:</li>
    <li><button>Export to PDF/Print</button></li>
  </ul>

  <div class="bracket" id="bracket4">
